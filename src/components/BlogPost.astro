---
import type { MarkdownPageProps } from "../layouts/MarkdownLayout.astro";

const { frontmatter } = Astro.props as { frontmatter: MarkdownPageProps };
console.log(frontmatter);
---

<a
  href={frontmatter.url}
  class="rounded-xl p-6 bg-white dark:bg-gray-900 shadow-gray-100 shadow-md dark:shadow-gray-800"
>
  <h1>{frontmatter.title}</h1>
  <p class="text-sm text-gray-500">
    {frontmatter.pubDate.toString().slice(0, 10)}
  </p>
</a>
